{
  "name": "@ndara/monorepo",
  "private": true,
  "description": "Package Templates Components for React",
  "keywords": [],
  "author": "Yofan Niki Andara Yudha",
  "license": "MIT",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "watch": "lerna run watch --scope ndaraui",
    "package": "lerna exec --scope ndaraui -- npm pack",
    "release:build": "lerna run build --scope ndaraui",
    "release:version": "lerna version --no-changelog --no-push --no-git-tag-version",
    "release:publish": "lerna publish --no-private",
    "start:example": "cd examples/example-app && yarn start"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "jest": "^28.1.1",
    "lerna": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
